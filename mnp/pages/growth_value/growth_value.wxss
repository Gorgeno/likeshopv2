/* pages/growth_value/growth_value.wxss */


.growth-value {
  background-image: url(data:image/png;base64,);
  background-size: 100% 213px;
  background-repeat: no-repeat;
  padding: 40rpx 24rpx 0;
  min-height: 100vh;
  box-sizing: border-box;
}



.growth-value .growth-list {
  padding: 0 30rpx;
  margin-top: 30rpx;
  border-radius: 10rpx;
}

.growth-value .growth-list .list-item {
  padding: 30rpx 0;
  border-bottom: var(--border);
}