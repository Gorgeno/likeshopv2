/* pages/user_vip/user_vip.wxss */
page {
    background-color: #fff;
}
.user-vip .header {
    background-image: url(data:image/png;base64,);
    padding-top: 30rpx;
    background-size: 100% 100%;
    height: 382rpx;
}

.user-vip .header swiper {
    height: 320rpx;
}

.user-vip .header .user-vip-info {
    padding-left: 30rpx;
}

.user-vip .header .user-vip-info .user-text {
    line-height: 50rpx;
    font-weight: bold;
}

.user-vip .header .user-vip-info .user-level {
    border: 1rpx solid white;
    border-radius: 100rpx;
    padding: 4rpx 20rpx;
    line-height: 30rpx;
}

.user-vip .content {
    margin-top: -240rpx;
}



.user-vip .content .vip-swiper-container .swiper .vip-card-item {
    height: 320rpx;
    width: 600rpx;
    position: relative;
    background-size: 100% 100%;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item .grade {
    line-height: 36rpx;
    background-color: rgba(0, 0, 0, 0.5);
    border-top-right-radius: 100rpx;
    border-bottom-right-radius: 100rpx;
    height: 50rpx;
    padding: 0 28rpx;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item .user-grade {
    line-height: 36rpx;
    margin-left: 30rpx;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item .grade-icon {
    width: 120rpx;
    height: 100rpx;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item .vip-name {
    padding: 10rpx 30rpx;
    font-size: 46rpx;
    text-align: center;
    align-items: flex-end;
    margin-bottom: 30rpx;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item .vip-progress {
    height: 8rpx;
    width: 540rpx;
}

.user-vip .content .vip-swiper-container .swiper .vip-card-item  .vip-progress .vip-progress-bar {
    background-color: #f8d07c;
    height: 100%;
}

.user-vip .content .vip-grade-rule {
    margin: 24rpx 40rpx;
}

.user-vip .content .vip-grade-rule .title .line {
    width: 8rpx;
    height: 34rpx;
    background-color: #f79c0c;
}



.user-vip .content .vip-grade-rule .rule-content ul li {
    list-style-type: decimal;
    font-size: 26rpx;
}

.user-vip .content .vip-grade-rule .rule-content ul li p {
    text-align: left;
    font-size: 26rpx;
    line-height: 36rpx;
    margin-bottom: 14rpx;
}

.user-vip .content .vip-rights {
    margin: 24rpx 40rpx;
}

.user-vip .content .vip-rights .title {
    padding: 28rpx 0;
}

.user-vip .content .vip-rights .title .line {
    width: 8rpx;
    height: 34rpx;
    background-color: #f79c0c;
}

.user-vip .content .vip-rights .rights-list {
    flex-wrap: wrap;
}

.user-vip .content .vip-rights .rights-list .rights-item {
    width: 25%;
    padding-bottom: 30rpx;
}

.user-vip .content .vip-rights .rights-list .rights-item image {
    width: 82rpx;
    height: 82rpx;
}